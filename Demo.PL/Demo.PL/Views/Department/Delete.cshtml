@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Are you Sure You want To Delete</h1>
<form asp-action="Delete"> 
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input type="text" class="form-control" asp-for="Code" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input type="text" class="form-control" asp-for="Name" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="DateOfCreation"></label>
        <input class="form-control" asp-for="DateOfCreation" readonly />
    </div>

    <input type="submit" class="btn btn-danger" value="Delete"/> |
    <a asp-action="Index" class="btn btn-light">Back</a>

</form>
